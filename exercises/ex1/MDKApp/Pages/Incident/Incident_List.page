{
	"Controls": [
		{
			"_Type": "Control.Type.SectionedTable",
			"_Name": "SectionedTable",
			"Sections": [
				{
					"Header": {
						"_Name": "SectionHeader0",
						"AccessoryType": "none",
						"UseTopPadding": false
					},
					"_Type": "Section.Type.ObjectTable",
					"Target": {
						"EntitySet": "Incident",
						"Service": "/MDKApp/Services/IncidentManagement.service",
						"QueryOptions": "$expand=customer&$filter=serviceWorker_ID eq 150"
					},
					"_Name": "SectionObjectTable0",
					"EmptySection": {
						"Caption": "No incident found!",
						"FooterVisible": false
					},
					"ObjectCell": {
						"ContextMenu": {
							"Items": [],
							"PerformFirstActionWithFullSwipe": true
						},
						"Title": "{Title}",
						"Subhead": "ID: {ID}",
						"Description": "{Description}",
						"StatusText": "{Urgency}",
						"StatusImage": "/MDKApp/Rules/Incident/ShowPriorityImage.js",
						"SubstatusText": "{Status}",
						"PreserveIconStackSpacing": false,
						"AccessoryType": "disclosureIndicator",
						"Tags": [],
						"AvatarStack": {
							"Avatars": [
								{
									"Image": "/MDKApp/Services/IncidentManagement.service/{@odata.readLink}/DeviceImage"
								}
							],
							"ImageIsCircular": true
						},
						"OnPress": "/MDKApp/Actions/Incident/NavToIncident_Detail.action"
					},
					"Search": {
						"Enabled": true,
						"Placeholder": "Incident Search",
						"BarcodeScanner": true,
						"Delay": 500,
						"MinimumCharacterThreshold": 3
					},
					"DataPaging": {
						"ShowLoadingIndicator": true,
						"LoadingIndicatorText": "Loading more items, please wait..."
					}
				}
			],
			"LoadingIndicator": {
				"Enabled": true,
				"Text": "Loading, please wait..."
			}
		}
	],
	"_Type": "Page",
	"_Name": "Incident_List",
	"Caption": "Incidents",
	"PrefersLargeCaption": true,
		"PullDown": {
		"OnPulledDown": "/MDKApp/Actions/Service/SyncStartedMessage.action"
	},
	"ActionBar": {
		"Items": [
			{
				"_Name": "ActionBarItem0",
				"Caption": "User",
				"Icon": "sap-icon://customer",
				"Position": "Right",
				"IsIconCircular": false,
				"Visible": true,
				"OnPress": "/MDKApp/Actions/Application/UserMenuPopover.action"
			}
		],
		"_Name": "ActionBar1"
	}
}